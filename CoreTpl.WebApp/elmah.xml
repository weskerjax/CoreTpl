<elmah>
  <errorFilter>
    <test>
      <or>        
        <equal binding="HttpStatusCode" value="404" type="Int32" />
				<is-type-compatible binding="BaseException" type="Orion.API.OrionException, Orion.API" />
      </or>
    </test>
  </errorFilter>
</elmah>